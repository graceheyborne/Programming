    "input_list = [1,2,3,4,5,6,7,8,9]\n",
    "threshold = 7\n",
    "\n",
    "def threshold_function(input_list, threshold):\n",
    "    new_list = []\n",
    "    for x in input_list:\n",
    "        if x <= threshold:\n",
    "            new_list.append(x)\n",
    "    return new_list\n",
    "\n",
    "threshold_function(input_list, threshold)"
